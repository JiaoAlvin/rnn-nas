{
  "env_name": "PTB",
  "persist": true,
  "slack_webhook": -1,
  "post_update_freq": 1,
  "restore_if_possible": true,


  "population_size": 100,
  "population_to_select": 100,
  "number_of_generations": 30,
  "initial_population_transformations_min": 1,
  "initial_population_transformations": 10,
  "override_transformation_count": 3,
  "warm_start_models": true,
  "include_lstm": false,
  "include_gru": false,
  "time_limit": -1,
  "alternative_nsga_crowding": true,
  "gate_unit_option": "limit_morphism",

  "_comment": "Configuration when training using the Penn Treebank dataset.",
  "ptb_training_epochs": 30,
  "ptb_ppl_upper_threshold": 500000,
  "ptb_model_nlayers": 1,
  "force_ptb_training": true,
  "ptb_compare_ppl_threshold": 5,
  "ptb_threshold_epochs": 5,
  "override_train_data": false,

  "_comment": "Configuration when training using the language dataset (regular languages).",
  "language_model_epochs": 35,
  "language_model_layers": 1,
  "language_model_loss": "MSELoss",
  "language_model_optimizer": "RMSprop",
  "language_model_learning_rate": "0.01",
  "language_model_sample_size": "500",
  "language_model_window_start": "1",
  "language_model_window_end": "10",
  "language_model_distrib_type": "uniform",
  "language_model_increase_language_size_freq": 999,
  "language_model_language": "abc",
  "language_loss_threshold": 0.005,

  "_comment": "Configuration when training using a sine wave dataset.",
  "sine_model_epochs": 3,
  "sine_model_input_size": 1,
  "sine_model_hidden_size": 51,
  "sine_model_loss": "MSELoss",
  "sine_model_optimizer": "LBFGS",
  "sine_model_learning_rate": "0.8",
  "sine_model_sample_size": "1",
  "sine_model_increase_phases_gen_freq": 99,
  "sine_model_increase_phases_amount": 1,
  "sine_model_increase_noise_gen_freq": 99,
  "sine_model_increase_noise_amount": 0.01,
  "sine_model_plot_gen_freq": 99,
  "sine_performance_count_threshold": 3,
  "sine_loss_threshold": 0.005,
  "sine_mae_threshold": 0.005,
  "sine_seconds_threshold": 2.0,
  "sine_model_phases": 10,
  "_sine_model_phases": 90,
  "sine_model_test_phases": 10,
  "sine_model_noise_range": 0.03,


  "objectives": [
    "number_of_functions",
    "ptb_ppl"
  ],
  "cheap_objectives": [
    "number_of_parameters",
    "number_of_functions"
  ],
  "default_persistence_columns": [
    "time",
    "model_id",
    "model_hash",
    "number_of_parameters",
    "training_time"
  ],
  "ptb_persistence_columns": [
    "ptb_ppl",
    "ptb_loss"
  ],
  "activation_functions": [
    "identity",
    "tanh",
    "sigmoid",
    "relu",
    "leaky_relu",
    "linear",
    "linear_b"
  ],
  "combination_methods": [
    "add",
    "elem_mul",
    "sub"
  ],
  "remove_transformations": [
    "remove_unit",
    "add_gate",
    "remove_connection"
  ],
  "network_transformations": [
    "add_unit",
    "add_connection",
    "add_recurrent_connection",
    "remove_unit",
    "remove_connection",
    "change_random_activation",
    "change_combination_connection"
  ],
  "identity": 0.2,
  "tanh": 0.2,
  "sigmoid": 0.2,
  "relu": 0.2,
  "linear": 0.2,
  "linear_b": 0.2,
  "None": 0.2,
  "add": 0.2,
  "elem_mul": 0.2,
  "sub": 0.2,
  "add_unit": 0.2,
  "remove_unit": 0.2,
  "add_gate": 0.2,
  "remove_gate": 0.02,
  "add_connection": 0.2,
  "remove_connection": 0.2,
  "add_recurrent_connection": 0.2,
  "remove_recurrent_connection": 0.2,
  "change_transfer_function": 0.2,
  "change_combination_connection": 0.2,
  "flip_network_type": 0.2,
  "flip_time_delay": 0.2,
  "flip_input": 0.2,
  "flip_output": 0.2,
  "clean_up": 0.2
}
